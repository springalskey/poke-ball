#!/usr/bin/env node

require('commander')
  .version(require('../package.json').version)
  .usage('<command> [options]')
  .command('new', 'new a vue project seed')
  .command('module', 'create module')
  .command('submodule', 'create submodule')
  .command('directive', 'create directive')
  .parse(process.argv);
