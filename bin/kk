#!/usr/bin/env node
var path = require('../tools/path');
console.log('CWD_PATH:',path.CWD_PATH);
console.log('CWD_PATH:',path.ROOT_PATH);
console.log('CWD_PATH:',path.SRC_PATH);

require('commander')
  .version(require('../package.json').version)
  .usage('<command> [options]')
  .command('new', 'new a vue project seed')
  .command('module', 'create module')
  .command('submodule', 'create submodule')
  .command('directive', 'create directive')
  .option('-p','CWD_PATH,ROOT_PATH,SRC_PATH')
  .parse(process.argv);
