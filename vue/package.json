  "license": "UNLICENSE",
  "homepage": "https://git.elenet.me/pcd/kingkong",
  "repository": {
    "type": "git",
    "url": "git@git.elenet.me:pcd/kingkong.git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --inline --hot --open",
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "stage-check": "if [[ ! -z $(git diff) ]]; then echo '✖ you should stage files first' && exit 1; fi ",
    "lint": "ADDED=$(git diff-index --cached HEAD --name-only --diff-filter ACMR | egrep '.(js|vue)$') eslint --ext js,vue ${ADDED:-src}",
    "fix": "eslint --fix --ext js,vue src; exit 0"
  },
  "pre-commit": {
    "run": [
      "stage-check",
      "lint"
    ],
    "silent": true
  },
  "dependencies": {
    "element-ui": "^1.0.0-rc.1",
    "vue": "^2.0.0-rc.3",
    "vue-resource": "^0.9.3",
    "vue-router": "^2.0.0-rc.3",
    "vuex": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-component": "^0.4.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint-plugin-vue": "^0.1.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.8.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.10.1",
    "pre-commit": "^1.1.3",
    "sass-loader": "^4.0.0",
    "standard": "^8.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.4.0",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0"
  }
}
